{
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules"
  ],
  "rules": {
    "no-unused-variable": true,
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "ter-indent": [
      true,
      2,
      {
        "FunctionExpression": {
          "parameters": 1,
          "body": 1
        }
      }
    ]
  }
}
