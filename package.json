  },
  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"nodemon dist/app.js\"",
    "tsc:w": "tsc -w",
    "test": "tsc & mocha server/tests/ --recursive --color --growl",
    "test:coverage": "rm -rf coverage/* && tsc && node --harmony_default_parameters ./node_modules/istanbul/lib/cli.js cover _mocha --harmony_default_parameters -- server/tests/ --recursive --color --growl",
    "postinstall": "tsc && npm run test:coverage"
  },
  "engines": {
    "node": ">= 5.4.1"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.3",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "express-validation": "^1.0.2",
    "express-winston": "^2.4.0",
    "helmet": "^3.5.0",
    "http-status": "^1.0.1",
    "joi": "^10.4.1",
    "method-override": "^2.3.8",
    "mockgoose": "^6.0.8",
    "mongoose": "^4.6.1",
    "morgan": "^1.8.1",
    "swagger-jsdoc": "^1.8.2",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.33",
    "@types/chai": "^3.4.33",
    "@types/chai-http": "0.0.29",
    "@types/compression": "^0.0.33",
    "@types/cookie-parser": "^1.3.30",
    "@types/express": "^4.0.33",
    "@types/joi": "^10.3.0",
    "@types/method-override": "^0.0.29",
    "@types/mocha": "^2.2.32",
    "@types/mongoose": "^4.5.34",
    "@types/swagger-jsdoc": "0.0.1",
    "@types/winston": "^2.3.1",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "concurrently": "^3.1.0",
    "istanbul": "^0.4.3",
    "mocha": "^2.5.3",
    "nodemon": "^1.10.0",
    "typescript": "^2.1.1"
  }
